%tr{id: "member_#{phone_number.id}"}
  - if @list.try(:all_users_can_send_messages) == false
    %td.toggle
      %div
        %input.adminToggle{type: 'checkbox', title: 'Toggle Admin', data: {list: @list.id, phone: phone_number.id}, checked: @list.number_is_admin?(phone_number) }
      %span Toggle Admin
  %td.name
    = link_to phone_number.contact.try(:full_name), @list ? edit_list_contact_path(@list, phone_number.contact) : edit_contact_path(phone_number.contact), class: 'clk'
  %td.num
    = number_to_phone(phone_number.number)
  - if @list
    %td.del
      %span.remove_member_icon
        = link_to_remove_member(phone_number, @list.id)